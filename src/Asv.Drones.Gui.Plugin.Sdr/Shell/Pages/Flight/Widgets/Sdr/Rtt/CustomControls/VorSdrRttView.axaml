    <Viewbox>
        <StackPanel>
            <indicators:DualCircularTripleSectionIndicator

                Title="FQ \ Power"

                TopRightProgressLinePoint="{Binding TopProgressLineRightPoint}"
                TopRightMax="120"
                TopRightMin="-20"
                TopRightProgressValue="{CompiledBinding PowerStringValue}"
                RightTopText="{CompiledBinding PowerStringValue}"
                RightTopTitle="{CompiledBinding PowerTitle}"
                RightTopStatus="Success"


                TopLeftProgressLinePoint="{Binding TopProgressLineLeftPoint}"
                TopLeftMin="-20"
                TopLeftMax="120"
                TopLeftProgressValue="{Binding FrequencyOffsetStringValue}"
                LeftTopText="{CompiledBinding FrequencyOffsetStringValue}"
                LeftTopTitle="{CompiledBinding FrequencyOffsetTitle}"
                LeftTopStatus="Success"


                LeftBottomLeftText="{CompiledBinding BearingStringValue}"
                LeftBottomLeftTitle="{CompiledBinding BearingTitle}"
                LeftBottomLeftStatus="Success"

                LeftBottomRightText="{CompiledBinding Am30HzStringValue}"
                LeftBottomRightTitle="{CompiledBinding Am30HzTitle}"
                LeftBottomRightStatus="Success"

                RightBottomLeftText="{CompiledBinding Am9960HzStringValue}"
                RightBottomLeftTitle="{CompiledBinding Am9960HzTitle}"
                RightBottomLeftStatus="Success"

                RightBottomRightText="{CompiledBinding FmDeviationStringValue}"
                RightBottomRightTitle="{CompiledBinding FmDeviationTitle}"
                RightBottomRightStatus="Success"

                BottomStatusText=""
                BottomStatus="Warning" />
        </StackPanel>
       
