<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:llz="clr-namespace:Asv.Drones.Gui.Plugin.Sdr.Rtt.Llz"
             xmlns:api="clr-namespace:Asv.Drones.Gui.Api;assembly=Asv.Drones.Gui.Api"
             mc:Ignorable="d" d:DesignWidth="50" d:DesignHeight="50"
             x:Class="Asv.Drones.Gui.Plugin.Sdr.Rtt.Llz.SdrRttItemLlzView"
             x:CompileBindings="True"
             x:DataType="llz:SdrRttItemLlzViewModel">
    <Design.DataContext>
        <llz:SdrRttItemLlzViewModelDesignMock/>
    </Design.DataContext>
    <api:DigitIndicator Margin="4,0" Title="{Binding Title}" FormatString="{Binding FormatString}" Units="{Binding Units}" Value="{Binding Value, Mode=TwoWay}"/>
</UserControl>
