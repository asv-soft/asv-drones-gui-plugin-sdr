<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             
             xmlns:customControls="clr-namespace:Asv.Drones.Gui.Plugin.Sdr.Rtt.CustomControls"
             xmlns:sdr="clr-namespace:Asv.Drones.Gui.Plugin.Sdr"
             xmlns:api="clr-namespace:Asv.Drones.Gui.Api;assembly=Asv.Drones.Gui.Api"
             mc:Ignorable="d" d:DesignWidth="700" d:DesignHeight="560"
             x:CompileBindings="True"
             x:DataType="customControls:LlzSdrRttViewModel"
             x:Class="Asv.Drones.Gui.Plugin.Sdr.Rtt.CustomControls.LlzSdrRttView">
    <Design.DataContext>
        <customControls:LlzSdrRttViewModel/>
    </Design.DataContext>
    <Viewbox>
                    <Canvas Background="Transparent" 
                            VerticalAlignment="Stretch" 
                            HorizontalAlignment="Stretch"
                            Width="700" Height="560">
                        <Grid Canvas.Top="67" Canvas.Left="7" 
                              Width="372" Height="137"
                              ColumnDefinitions="0.15*,0.3*,0.8*,0.3*"
                              RowDefinitions="1*,1*,1*,1*,0.8*">
                            <TextBlock Text="{x:Static sdr:RS.SdrRttViewModel_Rf_Header}"
                                       Grid.Column="0" Grid.Row="0"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Left" 
                                       Margin="5,0" 
                                       FontSize="22" 
                                       Foreground="SteelBlue" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding TotalPowerTitle}"
                                       Grid.Column="0" Grid.Row="1"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Left" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       Foreground="SteelBlue" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding CrsPowerTitle}"
                                       Grid.Column="0" Grid.Row="2"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Left" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       Foreground="DarkGreen" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding ClrPowerTitle}"
                                       Grid.Column="0" Grid.Row="3"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Left" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       Foreground="SaddleBrown"
                                       FontWeight="Bold"/>
                            
                            <TextBlock Text="{CompiledBinding TotalPowerUnits}"
                                       Grid.Column="1" Grid.Row="0"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Center" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding TotalPowerStringValue}"
                                       Grid.Column="1" Grid.Row="1"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Right" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding CrsPowerStringValue}"
                                       Grid.Column="1" Grid.Row="2"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Right" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding ClrPowerStringValue}"
                                       Grid.Column="1" Grid.Row="3"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Right" 
                                       Margin="5,0" 
                                       FontSize="16"
                                       FontFamily=""
                                       FontWeight="Bold"/>
                            
                            <api:LevelIndicator Margin="5,0" 
                                Grid.Column="2" Grid.Row="1"
                                Foreground="SteelBlue" 
                                VerticalAlignment="Stretch" 
                                HorizontalAlignment="Stretch"
                                ValueFrom="-120"
                                ValueTo="20"
                                Value="{CompiledBinding TotalPowerValue}"/>
                            <api:LevelIndicator Margin="5,0"
                                Grid.Column="2" Grid.Row="2"
                                Foreground="DarkGreen" 
                                VerticalAlignment="Stretch" 
                                HorizontalAlignment="Stretch"
                                ValueFrom="-120"
                                ValueTo="20"
                                Value="{CompiledBinding CrsPowerValue}"/>
                            <api:LevelIndicator Margin="5,0"
                                Grid.Column="2" Grid.Row="3" 
                                Foreground="SaddleBrown" 
                                VerticalAlignment="Stretch" 
                                HorizontalAlignment="Stretch"
                                ValueFrom="-120"
                                ValueTo="20"
                                Value="{CompiledBinding ClrPowerValue}"/>
                            
                            <TextBlock Text="{CompiledBinding CrsFrequencyUnits}"
                                       Grid.Column="3" Grid.Row="0"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Center" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding CrsFrequencyStringValue}"
                                       Grid.Column="3" Grid.Row="2"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Right" 
                                       Margin="5,0" 
                                       FontSize="16" 
                                       FontWeight="Bold"/>
                            <TextBlock Text="{CompiledBinding ClrFrequencyStringValue}"
                                       Grid.Column="3" Grid.Row="3"
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Right" 
                                       Margin="5,0" 
                                       FontSize="16"
                                       FontWeight="Bold"/>
