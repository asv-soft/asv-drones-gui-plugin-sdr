                        <Border  Background="Transparent"
                                 Width="696" Height="58"
                                 BorderBrush="SteelBlue"
                                 BorderThickness="2"
                                 Opacity="0.8"
                                 CornerRadius="0"
                                 Padding="0" Canvas.Top="2" Canvas.Left="2" />
                        
                        <Border  Background="Transparent"
                                 Width="382" Height="147"
                                 BorderBrush="SteelBlue"
                                 BorderThickness="2"
                                 Opacity="0.8"
                                 CornerRadius="0"
                                 Padding="0" Canvas.Top="62" Canvas.Left="2" />
                        <Border  Background="Transparent"
                                 Width="312" Height="147"
                                 BorderBrush="SteelBlue"
                                 BorderThickness="2"
                                 Opacity="0.8"
                                 CornerRadius="0"
                                 Padding="0" Canvas.Top="62" Canvas.Right="2" />
                        <Border  Background="Transparent"
                                 Width="696" Height="347"
                                 BorderBrush="SteelBlue"
                                 BorderThickness="2"
                                 Opacity="0.8"
                                 CornerRadius="0"
                                 Padding="0" Canvas.Bottom="2" Canvas.Left="2" />
                    </Canvas>
                </Viewbox>
</UserControl>
