                                        <TextBlock Grid.Column="0" Grid.Row="0" FontSize="20" Text="{x:Static sdr:RS.SdrRttViewModel_Main_Header}" Foreground="SteelBlue" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Left"/>
                                        
